generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Place {
  id String  @id @default(uuid())
  name String
  type String
  description String
  coordinates Json @unique
  contacts  Json
  logo String
  images String[]
}

model Users {
  id String @id @default(uuid())
  email String @unique
  password String?
  googleId String? @unique
  role RoleUsers @default(TURISTA)
}

enum RoleUsers {
  ADMIN
  TURISTA
}  